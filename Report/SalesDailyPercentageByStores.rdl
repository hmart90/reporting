  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DB</DataSourceName>
        <CommandText>/****** Script for SelectTopNRows command from SSMS  ******/
SELECT st.[Local Store Store name]
      ,st.[Local Store format 2] as StoreType
	  ,dailysales.[Day] as SalesDate
	  ,startingstock.TotalStock + startingsales.TotalSales  as StartingStockValue
	  ,dailysales.TotalSales as TotalSalesValue
	  ,(dailysales.TotalSales)/(startingstock.TotalStock + startingsales.TotalSales)  as SoldPercentage

FROM [Database].[dbo].[Store] as st

LEFT JOIN (
		SELECT	[Local Store Store code]
				,[Day]
				,sum(mstr.[Sold units]*pm.[DCMS beszerzési ár Beszerzési ár (nettó)]) as  TotalSales
		FROM [dbo].[SalesMSTR] as mstr
		INNER JOIN dbo.PriceMaster AS pm ON pm.TPN = mstr.[Local TPN item TPN]
		GROUP BY [Local Store Store code],[DAY]
) as dailysales on dailysales.[Local Store Store code] = st.[Local Store Store code]

LEFT JOIN (
		SELECT [Local Store Store code]
				,sum(mstr.[Stock units (Daily Shops)]*pm.[DCMS beszerzési ár Beszerzési ár (nettó)]) as  TotalStock
		FROM [dbo].[StockMSTR] as mstr
		INNER JOIN dbo.PriceMaster AS pm ON pm.TPN = mstr.[Local TPN item TPN]
		WHERE [Day] = '2018-12-17'
		GROUP BY [Local Store Store code]
) as startingstock on startingstock.[Local Store Store code]=st.[Local Store Store code]
LEFT JOIN (
					SELECT 
							[Local Store Store code]
							,sum(mstr.[Sold units]) as  TotalSales
					FROM [dbo].[SalesMSTR] as mstr
					WHERE [Day] &lt;= '2018-12-17'
					GROUP BY [Local Store Store code]
		) as startingsales on startingsales.[Local Store Store code] = st.[Local Store Store code]

where st.[Connection_112] = 1

ORDER BY dailysales.TotalSales/startingstock.TotalStock desc</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
